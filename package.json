{
	"name": "raw-nodejs-server",
	"description": "raw nodejs server with mongodb without any frameworks",
	"version": "1.0.0",
	"main": "build/app/index.js",
	"type": "module",
	"scripts": {
		"dev": "npx nodemon --env-file=.env ./build/app/index.js",
		"start": "node --env-file=.env ./build/app/index.js",
		"build": "npx tsc -p ./tsconfig.json",
		"typedev": "npx tsc -p ./tsconfig.json --watch"
	},
	"license": "CC0-1.0",
	"author": {
		"name": "MiraSayon",
		"url": "https://github.com/mirasayon"
	},
	"dependencies": {
		"mongoose": "^8.4.1"
	},
	"devDependencies": {
		"@types/node": "^20.12.12",
		"nodemon": "^3.1.3",
		"typescript": "^5.4.5"
	}
}
